    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "dist",
    "paths": {
      "*": ["node_modules/*"]
    },
    "sourceMap": true,
    "target": "es6",
  },
  "include": ["server/**/*"]
}
